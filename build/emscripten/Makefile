# Alternative GNU Make workspace makefile autogenerated by Premake

ifndef config
  config=debug
endif

ifndef verbose
  SILENT = @
endif

ifeq ($(config),debug)
  SFML_GRAPHICS_config = debug

else ifeq ($(config),release)
  SFML_GRAPHICS_config = release

else
  $(error "invalid configuration $(config)")
endif

PROJECTS := SFML_GRAPHICS

.PHONY: all clean help $(PROJECTS) 

all: $(PROJECTS)

SFML_GRAPHICS:
ifneq (,$(SFML_GRAPHICS_config))
	@echo "==== Building SFML_GRAPHICS ($(SFML_GRAPHICS_config)) ===="
	@${MAKE} --no-print-directory -C . -f SFML_GRAPHICS.make config=$(SFML_GRAPHICS_config)
endif

clean:
	@${MAKE} --no-print-directory -C . -f SFML_GRAPHICS.make clean

help:
	@echo "Usage: make [config=name] [target]"
	@echo ""
	@echo "CONFIGURATIONS:"
	@echo "  debug"
	@echo "  release"
	@echo ""
	@echo "TARGETS:"
	@echo "   all (default)"
	@echo "   clean"
	@echo "   SFML_GRAPHICS"
	@echo ""
	@echo "For more information, see https://github.com/premake/premake-core/wiki"